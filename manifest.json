{
  "name": "Colorful Bookmarks",
  "version": "1.0",
  "description": "Show bookmarks in new tab.",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "chrome_url_overrides": {
      "newtab": "newTab.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "chrome://favicon/"
  ],
  "manifest_version": 2
}
